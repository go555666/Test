<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="article">

	<select id="selArticleListCnt" parameterType="java.util.Map">
		<![CDATA[
			SELECT COUNT(*)
			FROM ARTICLE_MAST AM
		]]>
	</select>

	<select id="selArticleList" parameterType="java.util.Map">
		<![CDATA[
			SELECT *
			FROM ARTICLE_MAST AM
		]]>
	</select>
</mapper>
